<h1>Add New Admin</h1>
<%= form_with url: create_admin_admin_users_path, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit 'Create Admin' %>
  </div>
<% end %>
<%= link_to 'Back', admin_users_path %>
